{
  "private": true,
  "name": "code2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender --no-service-worker --template src/index.html --dest ../build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "prettier": {
      "trailingComma": "es5",
      "tabWidth": 2,
      "semi": false,
      "singleQuote": true
  },
  "devDependencies": {
    "dynamic-cdn-webpack-plugin": "^3.4.1",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "if-env": "^1.0.0",
    "module-to-cdn": "^3.1.2",
    "preact-cli": "^2.0.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "decko": "^1.2.0",
    "history": "^4.9.0",
    "node-sass": "^4.11.0",
    "preact": "^8.4.2",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.6.1",
    "redux-zero": "^5.0.2",
    "sass-loader": "^7.1.0"
  }
}
